<div class="js__parallax-window1 main-banner1" style="background: url('../../Images/1920x1080/Scaf1.jpg') 50% 0 no-repeat fixed;">
    <div class="parallax-content container" style="padding-top:120px !important;padding-bottom:100px !important;">
        <div class="col-md-7 carousel-fx">
            <h1 class="carousel-title wow fadeInUp animated" data-animation="FadeIn" data-wow-duration="1000ms" data-wow-delay="1000ms" style="text-shadow: 1px 3px 5px #000;color:#fff;margin-bottom:0">
            @DSL.Resources.Resource.ScaffoldingBannerTitle</h1>
            <hr style="border-color:#e74336;" />
            <p class="color-white wow fadeInUp animated" data-animation="FadeIn" data-wow-duration="1000ms" data-wow-delay="1500ms" style="text-shadow: 1px 2px 5px #000;text-indent:0;padding-left:5px;font-size:18px;color:#fff!important;">
               @DSL.Resources.Resource.ScaffoldingBannerText
            </p>
            <a href="@Url.Action("contacts", "Home")">
                <div class="btn-theme btn-theme-sm btn-white-brd text-uppercase wow fadeInDown animated" data-animation="FadeIn" data-wow-duration="1000ms" data-wow-delay="2000ms">@DSL.Resources.Resource.SliderContactButton</div>
            </a>
        </div>
    </div>
</div>
