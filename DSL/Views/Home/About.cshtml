@{
    ViewBag.Title = DSL.Resources.Resource.About1SectionTitle;
}
<div class="js__parallax-window1 main-banner" style="background: url('../../Images/1920x1080/career1.jpg') 50% 0 no-repeat fixed;">
    <div class="parallax-content container" style="padding-top:120px !important;padding-bottom:100px !important;">
        <div class="col-md-6 col-sm-6 carousel-fx">
            <h1 class="carousel-title margin-l-10 wow fadeInUp animated" data-animation="FadeInup" data-wow-duration="1000ms" data-wow-delay="1000ms" style="text-shadow: 1px 3px 5px #000;margin-bottom:0;">
            @DSL.Resources.Resource.AboutBannerTitle</h1>
            <hr style="border-color:#e74336;margin-top:0" />
            <p class="color-white wow margin-l-10 fadeInUp animated" data-animation="FadeInup" data-wow-duration="1000ms" data-wow-delay="1500ms" style="text-shadow: 1px 2px 5px #000;text-indent:0;font-size:18px;color:white!important;">
               @DSL.Resources.Resource.AboutBannerText
            </p>
        </div>
    </div>
</div>
<!--========== PARALLAX ==========-->
<!--========== PAGE LAYOUT ==========-->
<!-- About -->
<div class="content-lg container" style="padding-bottom:0;">
    
    <!--// end row -->

    <div class="row">
        <div class="col-sm-3">
            <div class="wow fadeInLeft animated" data-wow-duration="800ms" data-wow-delay="200ms">
                <img class="img-responsive" src="~/Images/kassym.jpg" alt="" style="margin-top:30px;">
                <p style="font-style:italic;margin-bottom:0;"><br />
                @DSL.Resources.Resource.AboutFounder1</p>
            </div>
        </div>
        <div class="col-sm-6 sm-margin-b-30">
            <div class="wow fadeInUp animated" data-wow-duration="800ms" data-wow-delay="200ms">
                <h3 class="color-red" style="margin-bottom:0;">@DSL.Resources.Resource.About1SectionTitle</h3>
                <hr style="margin-top:0; border-top:1px solid #b0b0b0;" />
                <div class="margin-b-30">
                    <p style="color:#000;">
                        @DSL.Resources.Resource.About1SectionTextSpan @DSL.Resources.Resource.About1SectionText1
                        <a href="http://lancasterholding.com/" class="footer-list-link" target="_blank" style="text-decoration:none!important;color:#007bbc!important">
                        Lancaster Group Kazakhstan</a> @DSL.Resources.Resource.About1SectionTextAnd Deborah Services International (@DSL.Resources.Resource.About1SectionText2
                        <a href="http://actavo.com/"  target="_blank" class="footer-list-link"style="text-decoration:none!important;color:#007bbc!important"> Actavo Group Limited, UK</a>)
                        @DSL.Resources.Resource.About1SectionText3</p>
                    <p style="color:#000;">
                      @DSL.Resources.Resource.About1SectionText4</p>
                    <p style="color:#000;">
                        @DSL.Resources.Resource.About1SectionTextSpan1 @DSL.Resources.Resource.About1SectionText5</p>
                </div>
            </div>
            </div>
        <div class="col-sm-3">
            <div class="wow fadeInRight animated" data-wow-duration="800ms" data-wow-delay="200ms">
                <img class="img-responsive" src="~/Images/Price.jpg" alt="" style="margin-top:30px;">
                <p style="font-style:italic;"> <br />@DSL.Resources.Resource.AboutFounder2</p>
            </div>
            </div>
    </div>

    <!--// end row -->
</div>
<!-- End About -->

<div class="promo-section overflow-h">
    <div class="content-lg container" style="padding-bottom:20px; padding-top:0; margin-top: 0;">
        <div class="col-md-7 sm-margin-b-50">
            <div class="wow fadeInLeft animated" data-wow-duration="800ms" data-wow-delay="200ms">
                <div class="margin-b-30">
                    <h3 class="color-red" style="margin-bottom:0;">@DSL.Resources.Resource.About2SectionTitle</h3>
                    <hr style="margin-top:0; border-top:1px solid #b0b0b0;" />
                    <p style="color:#000;">@DSL.Resources.Resource.About1SectionTextSpan1
                    @DSL.Resources.Resource.About2SectionText1 </p>
                    <p style="text-indent:0;margin-bottom:0;">
                        @DSL.Resources.Resource.About2SectionText2
                        <ul>
                            <li style="color:red;"><span style="font-size:15px;color:#585858;">@DSL.Resources.Resource.About2SectionText3 </span></li>
                            <li style="color:red;"><span style="font-size:15px;color:#585858;">@DSL.Resources.Resource.About2SectionText4   </span></li>
                            <li style="color:red;"><span style="font-size:15px;color:#585858;">@DSL.Resources.Resource.About2SectionText5 </span></li>
                        </ul>
                    </p>
                </div>
                </div>
            </div>
        <div class="col-md-5 sm-margin-b-50" style="padding-left:30px;">
            <div class="wow fadeInRight animated" data-wow-duration="800ms" data-wow-delay="200ms">
                <div class="margin-b-30">
                    <h3 class="color-red" style="margin-bottom:0;">@DSL.Resources.Resource.About3SectionTitle</h3>
                    <hr style="margin-top:0; border-top:1px solid #b0b0b0;" />
                    <p style="color:#000;">
                     @DSL.Resources.Resource.About3SectionText1</p>
                    <p style="text-indent:0;">
                        <ul>
                            <li style="color:red;"><span style="font-size:15px;color:#585858;">@DSL.Resources.Resource.About3SectionText2</span></li>
                            <li style="color:red;"><span style="font-size:15px;color:#585858;">@DSL.Resources.Resource.About3SectionText3</span></li>
                            <li style="color:red;"><span style="font-size:15px;color:#585858;">@DSL.Resources.Resource.About3SectionText4</span></li>
                            <li style="color:red;"><span style="font-size:15px;color:#585858;">@DSL.Resources.Resource.About3SectionText5</span></li>
                        </ul>
                    </p>
                </div>
            </div>
        </div>
    </div>
</div>




<!-- End Service -->
<div class="container-full-width">
    <div class="js__parallax-window" style="background: url('../../Images/1920x1080/parallax.jpg') 50% 0 no-repeat fixed;">
        <div style="background:rgba(0,0,0,0.45);">
            <div class="parallax-content container">
                <h3 class="color-white">@DSL.Resources.Resource.AboutCustomerReviewTitle</h3>
